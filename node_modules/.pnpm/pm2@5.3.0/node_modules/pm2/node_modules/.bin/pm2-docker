#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/pm2@5.3.0/node_modules/pm2/bin/node_modules:/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/pm2@5.3.0/node_modules/pm2/node_modules:/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/pm2@5.3.0/node_modules:/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/pm2@5.3.0/node_modules/pm2/bin/node_modules:/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/pm2@5.3.0/node_modules/pm2/node_modules:/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/pm2@5.3.0/node_modules:/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/pm2-docker" "$@"
else
  exec node  "$basedir/../../bin/pm2-docker" "$@"
fi
