{
	"name": "bakosafe-socket-server",
	"description": "An express server with socket.io for bakosafe to emmit events to the all ecosystem",
	"author": "Infinitybase - Guilherme Roque",
	"license": "Apache-2.0",
	"version": "0.0.1",
	"scripts": {
		"dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register -r dotenv/config src/index.ts",
		"start": "node ./build/index.js",
		"build": "tsc --project . && tscpaths -p tsconfig.json -s ./src -o ./build",
		"run:socket-server": "make -C ./ deploy env_file=.env"
	},
	"keywords": [],
	"dependencies": {
		"axios": "1.5.1",
		"bakosafe": "0.0.94-beta",
		"express": "4.17.1",
		"express-joi-validation": "5.0.0",
		"fuels": "0.96.1",
		"pg": "8.5.1",
		"socket.io": "4.7.2",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^3.15.0",
		"typescript": "~5.4.5"
	},
	"devDependencies": {
		"@commitlint/cli": "12.0.1",
		"@commitlint/config-conventional": "12.0.1",
		"@trivago/prettier-plugin-sort-imports": "2.0.2",
		"@types/cors": "2.8.10",
		"@types/express": "4.17.11",
		"@types/jest": "^29.5.12",
		"@types/jsonwebtoken": "9.0.2",
		"@types/morgan": "1.9.2",
		"@types/node": "20.6.0",
		"@types/pg": "^8.11.6",
		"@types/supertest": "2.0.10",
		"@typescript-eslint/eslint-plugin": "6.5.0",
		"@typescript-eslint/parser": "6.5.0",
		"eslint": "7.22.0",
		"eslint-config-prettier": "8.1.0",
		"eslint-plugin-prettier": "3.3.1",
		"husky": "5.2.0",
		"jest": "29.7.0",
		"lint-staged": "10.5.4",
		"prettier": "2.2.1",
		"pretty-quick": "3.1.0",
		"supertest": "6.1.3",
		"ts-jest": "^29.1.3",
		"ts-node-dev": "1.1.6",
		"tscpaths": "0.0.9"
	}
