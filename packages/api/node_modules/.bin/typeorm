#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/typeorm@0.2.45_pg@8.5.1/node_modules/typeorm/node_modules:/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/typeorm@0.2.45_pg@8.5.1/node_modules:/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/typeorm@0.2.45_pg@8.5.1/node_modules/typeorm/node_modules:/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/typeorm@0.2.45_pg@8.5.1/node_modules:/Users/guimroque/Infinity/bsafe/bsafe-api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/typeorm@0.2.45_pg@8.5.1/node_modules/typeorm/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/typeorm@0.2.45_pg@8.5.1/node_modules/typeorm/cli.js" "$@"
fi
